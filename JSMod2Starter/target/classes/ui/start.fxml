<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="pane" prefHeight="412.0" prefWidth="621.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cn.jsmod2.panel.StartController">
    <children>
      <TabPane prefHeight="412.0" prefWidth="621.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
            <Tab fx:id="console" text="Console">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TextArea fx:id="consoleTextArea" layoutY="29.0" prefHeight="354.0" prefWidth="621.0" />
                        <TextField fx:id="lineHeightTextField" layoutX="14.0" layoutY="3.0" text="100" />
                        <CheckBox fx:id="scollCheck" layoutX="321.0" layoutY="6.0" mnemonicParsing="false" text="滚动设置" onAction="#scollCheck"/>
                        <Button fx:id="lineHeightSettingButton" layoutX="182.0" layoutY="3.0" mnemonicParsing="false" text="设置日志留存长度" onAction="#settingLineHeight" />
                     </children>
                  </AnchorPane>
              </content>
            </Tab>
          <Tab fx:id="server" text="Server">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
          <Tab fx:id="players" text="Players">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
            <Tab fx:id="game" text="Game">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
              </content>
            </Tab>
        </tabs>
      </TabPane>
    </children>
</AnchorPane>
